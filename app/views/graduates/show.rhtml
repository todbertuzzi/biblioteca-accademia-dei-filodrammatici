
<%= image_tag url_for_file_column(@graduate, "image") if @graduate.image %>
<% for column in Graduate.content_columns %>
<p>
  <b><%= column.human_name %>:</b> <%= @graduate.send(column.name) %>
</p>
<% end %>

<%= link_to 'Edit', :action => 'edit', :id => @graduate %> |
<%= link_to 'Back', :action => 'list' %>
